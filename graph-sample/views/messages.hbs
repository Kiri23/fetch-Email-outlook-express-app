<!-- <LayoutSnippet> -->
<h1 class="mb-3">Messages</h1>
<table class="table">
  <thead>
    <tr>
      <th scope="col">Subject</th>
    </tr>
  </thead>
  <tbody>
    {{#each messages}}
      <tr>
        <td>{{this.subject}}</td>
      </tr>
    {{/each}}
  </tbody>
</table>
<!-- </LayoutSnippet> -->
